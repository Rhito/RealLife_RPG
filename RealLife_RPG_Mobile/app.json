{
  "expo": {
    "name": "RealLife RPG",
    "slug": "RealLife_RPG_Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-logo.jpg",
    "scheme": "realliferpgmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ritoki133.RealLifeRPG"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/app-logo.jpg",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "statusBar": {
        "barStyle": "light-content",
        "backgroundColor": "#432874"
      },
      "usesCleartextTraffic": true,
      "package": "com.ritoki133.RealLife_RPG_Mobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/app-logo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "@react-native-community/datetimepicker",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7695f86b-84a2-4b3f-8fa9-489de3a6dba6"
      }
    },
    "owner": "ritoki133"
  }
}
